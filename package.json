{
  "name": "random-link-opener",
  "description": "Bookmark Links and open them when you need it, using a keyboard shortcut..",
  "private": true,
  "version": "1.2.2",
  "license": "MIT",
  "homepage": "https://github.com/AlanD20/random-link-openers",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "bundle": "rm -rf ./bundle.zip ./dist && yarn build && cd ./dist && zip -r bundle.zip * && mv ./bundle.zip ..",
    "win:bundle": "rm -rf ./bundle.zip ./dist && yarn build",
    "preview": "vite preview",
    "format": "prettier -w src",
    "prepare": "husky install && chmod ug+x .husky/pre-commit"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.3",
    "daisyui": "^2.18.0",
    "nanoid": "^4.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^1.0.12",
    "@tailwindcss/typography": "^0.5.2",
    "@types/chrome": "^0.0.191",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.12",
    "husky": "^8.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.5.0"
  }
}
